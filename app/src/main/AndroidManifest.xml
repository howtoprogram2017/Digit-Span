<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.digit_span">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DigtSpanActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".VisTestEntranceActivity"/>
        <activity android:name=".AudiTestEntranceActivity"
            android:launchMode="singleTop"/>
        <activity android:name=".AuditoryActivity"
            android:label="Auditory Forward Pretest"
            android:screenOrientation="portrait"/>
        <provider
            android:authorities="com.example.android.digit_span"
            android:name=".data.TestProvider"
            android:exported="false"/>
    </application>



</manifest>